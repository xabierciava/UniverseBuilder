<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.universes.universebuilder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UniverseBuilder">
        <activity android:name="com.universes.universebuilder.VerPagina"></activity>
        <activity android:name="com.universes.universebuilder.VerUniverso" />
        <activity android:name="com.universes.universebuilder.ElegirPagina" />
        <activity android:name="com.universes.universebuilder.EditarPagina" />
        <activity android:name="com.universes.universebuilder.PantallaCarga">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.universes.universebuilder.NuevoUniverso"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.universes.universebuilder.EditarUniverso" />
        <activity android:name="com.universes.universebuilder.ListaExplorar" />
        <activity android:name="com.universes.universebuilder.ListaMisMundos" />
        <activity android:name="com.universes.universebuilder.MenuPrincipal" />
        <activity
            android:name="com.universes.universebuilder.Entrar"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.universes.universebuilder.Registrar"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name="com.universes.universebuilder.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>